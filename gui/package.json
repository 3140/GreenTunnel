{
  "name": "green-tunnel-gui",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "test": "electron .",

    "generate-icons": "electron-icon-maker --input=./icon.png --output=.",

    "package-mac": "electron-packager . 'Green Tunnel' --overwrite --asar --platform=darwin --arch=x64 --icon=./icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar --platform=win32 --arch=ia32 --icon=./icons/win/icon.ico --prune=true --out=release-builds",
    "package-linux": "electron-packager . --overwrite --asar --platform=linux --arch=x64 --icon=./icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^4.1.0",
    "electron-icon-maker": "0.0.4",
    "electron-packager": "^13.1.1"
  },
  "dependencies": {
    "electron-window-state": "^5.0.3",
    "green-tunnel": "^1.0.2",
    "jquery": "^3.3.1"
  }
}
